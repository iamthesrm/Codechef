#include<bits/stdc++.h>
using namespace std;

int main(){
#ifdef LOCAL_PROJECT
  	freopen("a.in","r",stdin);
#else
#endif
	ios::sync_with_stdio(0);
	cin.tie(NULL);
	
	int x;
	cin >> x;
	while(x--){
		int a, b, c=0, d;
		cin >> a;
		int x[a];
		for(int i=0; i<a; i++)
			cin >> x[i];
		for(int i=0; i<a; i++)
			if(x[i]>=1000)
				c++;
		cout << c << endl;
	}
}
