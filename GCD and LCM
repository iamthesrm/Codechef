#include<bits/stdc++.h>
using namespace std;

int main(){
#ifdef LOCAL_PROJECT
  	freopen("a.in","r",stdin);
#else
#endif
	ios::sync_with_stdio(0);
	cin.tie(NULL);
	
	int t;
	cin >> t;
	while(t--){
		int a, b, c, d;
		cin >> a >> b;
		c = min(a,b);
		for(int i=1; i<=c; i++){
			if(a%i==0 && b%i==0)
				d = i;
			}
		cout << d << " " << long((long)a*(long)b)/d << endl;
	}
	return 0;
}
